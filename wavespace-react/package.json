{
  "name": "wavespace-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "deploy": "node scripts/deploy.js",
    "deploy:dev": "VITE_ENV=development npm run deploy",
    "deploy:staging": "VITE_ENV=staging npm run deploy", 
    "deploy:prod": "VITE_ENV=production npm run deploy",
    "lighthouse": "lighthouse-ci autorun",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "clean": "rm -rf dist coverage test-results .turbo",
    "setup": "npm install && npm run format && npm run lint:fix",
    "health-check": "node scripts/health-check.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.5",
    "@testing-library/react": "^16.2.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.2.0",
    "lighthouse": "^11.7.1",
    "@lhci/cli": "^0.12.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.8"
  }
}
